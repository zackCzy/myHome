}

.content_user_li {
	margin: 10px 0 0;
	float: left;
	width: 565px;
	height: auto;
	color: #808080;
	padding: 10px 15px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: break-word;
	font-size: 15px;
	margin-bottom: 9px;
	overflow: hidden;
	word-break: break-all;
}

.content_user_li div {
	margin: 10px 0 0;
