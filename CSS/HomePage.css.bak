	border-bottom:4px solid #DC3C00;
}
.music:hover strong {
	color: #9ED034;	
}

.music span,.music a,.music b {
	height: 20px;
	line-height: 20px;
	margin: 0;
}

.musicDisplay {
	position: absolute;
	width: 300px;
	height: 0px;
	background: url("/myHome/image/trans.png");
	left: -170px;
	top: -150px;
	box-shadow: 4px 3px 2px rgba(0, 0, 0, 0.03), 3px 2px 1px
		rgba(0, 0, 0, 0.4);
	overflow: hidden;
	z-index: 1;
	border: 1px solid #1B1E23;
	border-radius: 0 0 10px 10px;
}

#scrollBar {
	display: none;
	height: 260px;
	width: 5px;
	float: right;
	background: #1C1E24;
	margin-top: 10px;
	position: relative;
	border-radius: 5px;
}

#head_play_List {
	float: left;
	width: 260px;
	list-style: none;
	cursor: pointer;
	text-indent: 1em;
	color: #FBFBFB;
	scrollbar-arrow-color: #A9C50B;
	scrollbar-base-color: #A9C50B;
	scrollbar-dark-shadow-color: #A9C50B;
	scrollbar-face-color: #A9C50B;
	position: absolute;
	top: 0;
	left: 6px;
}

#head_list_display {
	width: 290px;
	margin: 10px 0 0 4px;
	overflow: hidden;
	height: 270px;
	position: relative;
}

#head_play_List li {
	width: 260px;
	height: 30px;
	margin: 4px 0;
	border-bottom: 1px dashed #474747;
	line-height: 30px;
	text-align: left;
	padding: 0;
}

#head_play_List li:hover {
	background: #5CAAE6;
}

.playList {
	width: 280px;
	margin: 3px auto;
}

.playList a,.playList b {
	float: right;
	color: #5CAAE6;
}

.playList a:LINK {
	float: right;
	color: #5CAAE6;
}

.small_speak_info {
	float: right;
	width: 250px;
	height: 200px;
	font-size: 15px;
	margin-right: 5px;
	white-space: nowrap;
	font-family: punctuation, 微软雅黑;
	font-weight: bold;
	color: #6C6351;
}

.small_speak_info span {
	height: 35px;
	width: 250px;
	overflow: hidden;
	text-indent: 25px;
	line-height: 50px;
	display: block;
}

.small_speak_info span a {
	color: #423009;
	font-size: 22px;
	text-decoration: none;
	margin-right: 10px;
}

.small_speak_info span a:hover {
	text-decoration: underline;
	color: #5094DB;
}

.small_speak_info img {
	width: 100px;
	height: 100px;
	border-radius: 100px;
	border: 2px solid #5CAAE6;
	box-shadow: 1px 4px 4px rgba(0, 0, 0, 0.05), 2px 3px 5px
		rgba(0, 0, 0, 0.3);
	margin: 0 15px;
	float: left;
}

.small_speak_info strong {
	height: 17px;
	margin: 15px 0;
	display: block;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.userNotice {
	width: 640px;
	height: 460px;
	background: #FFFFFF url("/myHome/image/userNotice.png") 30px no-repeat;
	display: block;
	margin: 0 0 0 15px;
	float: left;
	box-shadow: 3px 6px 4px rgba(0, 0, 0, 0.05), 4px 7px 5px
		rgba(0, 0, 0, 0.3);
	border: 2px solid #DCDCDC;
}

.userNotice h2 {
	color: #6C6C6C;
	margin: 40px auto;
	width: 500px;
	height: 40px;
	font-size: 25px;
	text-shadow: 3px 6px 4px rgba(0, 0, 0, 0.04), 4px 7px 5px
		rgba(0, 0, 0, 0.2);
}

.userNotice a {
	color: #3FA7CB;
	text-decoration: none;
}

.userNotice a:hover {
	text-decoration: underline;
}

.small_reply_input {
	width: 435px;
	padding: 15px;
	border: 1px solid #DEDEDE;
	font-size: 14px;
	line-height: 5px;
}

.small_reply_input:hover,.small_reply_input:focus {
	border: 1px solid #02ADE6;
}

.small_reply_area {
	margin: 7px 0 7px 48px;
	width: 468px;
	float: left;
	display: none;
}

.small_reply_bt {
	float: right;
	width: 80px;
	height: 20px;
	margin: 8px 0;
	background: #02ADE6;
	color: #DCDCDC;
	border-radius: 8px;
	cursor: pointer;
}