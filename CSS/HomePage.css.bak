}

#modify_user_photo {
	position: absolute;
	top: 0;
	margin-top: 150px;
	height: 25px;
	display: none;
	line-height: 25px;
	background: #ccc;
	width: 184px;
	text-align: center;
	font-size: 14px;
	color: #423009;
	background: url("/myHome/image/trans.png");
	text-decoration: none;
	font-weight: bold;
}

.user_page_photo li a {
	width: 60px;
	height: 70px;
	display: block;
	text-decoration: none;
	color: #423009;
}

.user_page_photo ul {
	list-style: none;
	display: block;
}

.user_page_photo li {
	width: 60px;
	height: 70px;
	float: left;
	cursor: pointer;
	text-align: center;
	font-size: 13px;
}

.user_page_photo li a:hover {
	color: #6C6351;
}

.home_content_display {
	width: 984px;
	border-top: 4px solid #5094DB;
