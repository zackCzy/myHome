	width: auto;
	height: auto;
	word-break: break-all;
	word-wrap: break-word;
}

.comment_box span a {
	color: #6C6351;
	text-decoration: none;
}

.con_user_box span {
	color: #6C6351;
	display: block;
	font-family: Georgia, Tahoma, Helvetica, arial;
	font-size: 12px;
