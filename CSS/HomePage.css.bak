@CHARSET "UTF-8";

body {
	background: #1F160D url("/myHome/image/body_bg.jpg") repeat-x;
}

.contentPoint {
	width: 220px;
	height: 130px;
	background: #EFEFEF url(/myHome/image/contentPoint.png);
	border: 3px solid #58CAFA;
	border-top: 0;
	display: none;
	text-align: center;
	line-height: 90px;
	height: 0;
	opacity: 0;
	filter: alpha(opacity =             0);
	z-index: 9889;
	border-radius: 0 0 8px 8px;
	text-shadow: 0 2px 3px #CFCFCF;
	box-shadow: 3px 3px 2px rgba(0, 0, 0, 0.05), 5px 5px 3px
		rgba(0, 0, 0, 0.1);
	font-size: 14px;
	color: #898989;
	font-weight: bold;
	position: fixed;
	top: 0;
}

.Home_page_nav {
	width: 1000px;
	height: 50px;
	background: #48525E;
	border-bottom: 4px solid #E1E1E1;
	margin-top: 30px;
	color: #dcdcdc;
	font-size: 15px;
	line-height: 50px;
	font-weight: bold;
	z-index: 9888;
	box-shadow: 0px 6px 4px rgba(0, 0, 0, 0.05), 0px 7px 5px
		rgba(0, 0, 0, 0.3);
}

.diray_list_ul {
	float: right;
	list-style: none;
	width: 150px;
	background: #FCFCFC;
	line-height: 45px;
	padding: 10px 15px;
	box-shadow: 2px 3px 4px 4px rgba(0, 0, 0, 0.05), 3px 4px 6px 6px
		rgba(0, 0, 0, 0.3);
	margin-right: 40px;
}

.diray_list_ul li {
	width: 150px;
	height: 45px;
	text-align: center;
	cursor: pointer;
	margin: 0;
}

.diray_list_ul a {
	width: 150px;
	height: 45px;
	display: block;
	text-decoration: none;
	color: #747474;
	border-radius: 15px;
	font-weight: bold;
	font-size: 14px;
}

.diray_list_ul li:hover {
	background: #FF644F;
	color: #5D9606;
	box-shadow: 2px 2px 1px rgba(0, 0, 0, 0.05), 4px 4px 2px
		rgba(0, 0, 0, 0.3);
	margin-left: -20px;
}

.Home_page_nav ul {
	list-style: none;
	width: 1000px;
}

.search_user_i {
	width: 165px;
	height: 15px;
	border: 1px solid #DCDCDC;
	border-radius: 15px;
	padding: 5px 0 5px 15px;
	margin: 13px;
	background: #ECECEC;
	float: left;
	-webkit-transition:box-shadow 0.3s inline;
	-moz-transition:box-shadow 0.3s inline;
	-o-transition:box-shadow 0.3s inline;
	-ms-transition:box-shadow 0.3s inline;
	transition:box-shadow 0.3s inline;
}
.search_user_i:hover{
	box-shadow: inset 0 1px 1px rgba(0,0,0,0.075),0 0 8px rgba(82,168,236,0.6);
	background: #ffffff;
}
#lock {
	display: none;
	position: absolute;
	background: #000;
	opacity: 0;
	filter: alpha(opacity =         0);
	z-index: 500;
}
#login_view {
	position: absolute;
	top: 50px;
	left: 200px;
	width: 330px;
	height: 300px;
	background: none repeat scroll 0 0 rgba(145, 105, 105, 0.86);
	z-index: 9999;
	box-shadow: 0 6px 4px rgba(0, 0, 0, 0.05), 0 7px 5px rgba(0, 0, 0, 0.3);
	border: 1px solid #a7a7a7;
	border-radius: 15px;
	color: #FCFCFC;
	display: none;
}
h4{
	height:25px;
}
#login_view #close_login {
	float: right;
	width: 26px;
	height: 26px;
	color: #FF4700;
	position: relative;
	right: 15px;
	top: 9px;
	margin: 0;
	padding: 0;
	line-height: 0;
	display: block;
	text-align: center;
	line-height: 26px;
	cursor: pointer;
	font-weight: 700;
}

#login_view #close_login:hover {
	color: #FF0000;
}

.pro-user-checkbox input {
	float: left;
	margin: 1px 3px 0 0;
}

#login_move {
	cursor: move;
}
.pro-user-checkbox {
	clear: both;
	color: #ECECEC;
	display: block;
	font: 12px/1.2em SimSun;
	margin-left: 35px;
}

#login_button {
	width: 200px;
	height: 35px;
	margin: 25px 55px;
	background: none repeat scroll 0 0 #A9C50B;
	cursor: pointer;
	font-size: 14px;
	vertical-align: middle;
	border-radius: 3px;
	font-family: inherit;
	color: #fff;
	letter-spacing: 1em;
	text-indent: 1em;
	text-shadow: 0 6px 4px rgba(0, 0, 0, 0.05), 0 7px 5px rgba(0, 0, 0, 0.3);
}

#miniMusic_userName,#miniMusic_password {
	width: 150px;
	height: 20px;
	padding: 8px 15px;
	font-size: 14px;
	float: left;
	line-height: 20px;
	margin-bottom: 25px;
	border: 1px solid #A9C50B;
	border-radius: 15px;
	background: none repeat scroll 0 0 rgba(145, 105, 105, 0.86);
	color: #FCFCFC;
}

#login_view input:focus,#login_view input:hover {
	box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset, 0 0 8px
		rgba(102, 175, 233, 0.6);
	border: 1px solid #90C1ED;
}

#login_view span {
	width: 70px;
	display: block;
	height: 36px;
	float: left;
	line-height: 36px;
	text-align: right;
	margin-right: 15px;
	margin-bottom: 25px;
}
.login_area{
	margin-right:25px;
}
.search_user_i span {
	background: url('/myHome/images/space/bg_line.png');
	width: 20px;
	height: 20px;
	display: block;
	float: left;
	margin: -2px 5px 0 0;
	cursor: pointer;
}



.search_user_i:hover input {
	background: #ffffff;
}

.search_user_i input {
	background: #ECECEC;
}

.user_message {
	float: right;
	width: 165px;
	height: 50px;
	text-align: right;
	font-size: 13px;
	line-height: 55px;
	color: #ffffff;

}
.user_message img,.user_message strong{
	margin:15px 0 0 10px;
	float: right;
	display: block;
	text-overflow: ellipsis;
	overflow: hidden;
}
.user_message strong{
	width:120px;
	margin: 0 0 0 10px;;
	text-align: left;
}
.user_message  a,.user_message  a:ACTIVE,.user_message  a:LINK,.user_message  a:VISITED
	{
	cursor: pointer;
	color: #ffffff;
	text-decoration: none;
}

.user_message  a:hover {
	color: #9ED034;
}

.Home_page_nav li {
	float: left;
	display: block;
	width: 70px;
	height: 50px;
	border-bottom: 4px solid #E1E1E1;
	text-align: center;
	line-height: 50px;
	margin-right: 18px;
	cursor: pointer;
}

.Home_page_nav li a,.Home_page_nav li a:VISITED,.Home_page_nav li a:LINK
	{
	display: block;
	width: 70px;
	height: 50px;
	cursor: pointer;
	text-decoration: none;
	color: #dcdcdc;
}

.Home_page_nav li:hover {
	border-bottom: 4px solid #DC3C00;
}

.mypage_img {
	margin-top: 20px;
	width: 1000px;
	height: 300px;
	background: url("/myHome/image/1.jpg");
	width: 1000px;
	display: block;
	position: relative;
}

.my_Info_display {
	width: 1000px;
	height: 130px;
	background: #DCDCDC url("/myHome/image/contentPoint.png");
	display: block;
	position: relative;
}

.user_page_photo {
	width: 180px;
	height: 250px;
	padding: 3px;
	background: #FAFAFA;
